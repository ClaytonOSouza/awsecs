{
    "family": "sitemeu",
    "containerDefinitions": [
        {
            "image": "%ACCOUNT%.dkr.ecr.us-east-1.amazonaws.com/%REPO%:%BUILD_TAG%",
            "name": "frontend",
            "essential": true,
            "cpu": 100,
            "memoryReservation": 300,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 80,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp"
                }
            ]
        }
    ]
}
