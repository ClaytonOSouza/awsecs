{
    "family": "sitemeu",
    "containerDefinitions": [
        {
            "image": "328527480917.dkr.ecr.us-east-1.amazonaws.com/sitemeu:%BUILD_TAG%",
            "name": "sitemeu",
            "essential": true,
            "cpu": 100,
            "memoryReservation": 300,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 80,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp"
                }
            ]
        }
    ]
}
