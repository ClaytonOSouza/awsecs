{
    "family": "sitemeu",
    "containerDefinitions": [
        {
            "image": "328527480917.dkr.ecr.us-east-1.amazonaws.com/REPO:BUILD_TAG",
            "name": "sitemeu",
            "essential": true,
            "cpu": 1000,
            "memoryReservation": 512,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 80,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp"
                }
             ]
        }
    ]
}
